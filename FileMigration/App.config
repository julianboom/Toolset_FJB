<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--数据库链接地址-->
    <add key="MySQLConn" value="Database='SjwFlowLocal';Data Source='localhost';Port='3306';User Id='root';Password='';charset='utf8';pooling=true;Allow User Variables=True;AutoEnlist=false" />
    <!--需要同步的文件所在的文件夹-->
    <add key="DirectoryPath" value="C:\file\sourceFile" />
    <!--文件移动后的文件夹-->
    <add key="RemoveFilePath" value="C:\file\removeFile" />
  </appSettings>
<system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.12.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data></configuration>